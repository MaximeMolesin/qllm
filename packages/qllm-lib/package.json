{
  "name": "qllm-lib",
  "version": "1.4.9",
  "private": false,
  "description": "Core library providing robust AI engineering functionalities tailored for Large Language Model (LLM) applications, enabling developers to build, deploy, and optimize AI solutions with ease.",
  "keywords": [
    "ai",
    "llm",
    "qllm",
    "library",
    "typescript",
    "aws-sdk"
  ],
  "type": "module",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/index.d.ts", 
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc --project tsconfig.esm.json && tsc --project tsconfig.cjs.json",
    "test": "jest",
    "lint": "eslint .",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf tsconfig.tsbuildinfo",
    "format": "prettier --write ."
  },
  "author": {
    "name": "QuantaLogic",
    "url": "https://quantalogic.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/quantalogic/qllm.git"
  },
  "bugs": {
    "url": "https://github.com/quantalogic/qllm/issues"
  },
  "homepage": "https://github.com/quantalogic/qllm#readme",
  "dependencies": {
    "@aws-sdk/client-sts": "^3.624.0",
    "@aws-sdk/credential-providers": "^3.624.0",
    "@aws-sdk/types": "^3.609.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.1.0",
    "@typescript-eslint/parser": "^8.1.0",
    "eslint": "^9.9.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.1.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  }
}
