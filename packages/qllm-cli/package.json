{
  "name": "qllm",
  "version": "1.4.0",
  "private": false,
  "description": "A versatile command line interface (CLI) for QLLM, enabling seamless interaction with multiple providers of Large Language Models (LLMs). Simplify your AI workflows with powerful commands and integrations.",
  "main": "dist/cli.js",
  "type": "module",
  "types": "dist/cli.d.ts",
  "bin": {
    "qllm": "dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev-cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "dev-server": "ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "test": "jest",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start": "node dist/src/server.js",
    "cli": "node dist/src/cli.js",
    "prune": "ts-prune prune",
    "lint": "eslint 'src/**/*.ts'",
    "format": "prettier --write ."
  },
  "dependencies": {
    "commander": "^9.2.0",
    "inquirer": "^8.1.0",
    "qllm-lib": "^1.4.0"
  },
  "keywords": [
    "llm",
    "cli",
    "ai",
    "command-line",
    "multi-provider"
  ],
  "author": {
    "name": "QuantaLogic",
    "url": "https://quantalogic.com"
  },
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/quantalogic/qllm"
  },
  "bugs": {
    "url": "https://github.com/quantalogic/qllm/issues"
  },
  "homepage": "https://github.com/quantalogic/qllm#readme"
}
