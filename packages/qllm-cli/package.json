{
  "name": "qllm",
  "version": "1.3.0",
  "description": "QLLM Multi-Provider LLM Command CLI",
  "main": "dist/qllm-cli/src/cli.js",
  "type": "module",
  "types": "dist/qllm-cli/src/cli.d.ts",
  "bin": {
    "qllm": "dist/qllm-cli/src/cli.js"
  },
  "outDir": "./dist",
  "scripts": {
    "dev-cli": "ts-node -r tsconfig-paths/register src/cli.ts",
    "dev-server": "ts-node -r tsconfig-paths/register src/server.ts",
    "build": "tsc",
    "test": "jest",
    "dev": "ts-node -r tsconfig-paths/register src/index.ts",
    "start": "node dist/src/server.js",
    "cli": "node dist/src/cli.js",
    "prune": "ts-prune prune",
    "lint": "eslint 'src/**/*.ts'"
  },
  "dependencies": {
    "@qllm/lib": "^1.3.0",
    "commander": "^9.2.0",
     "inquirer": "^8.1.0"
  },
  "keywords": [
    "llm",
    "cli",
    "ai"
  ],
  "author": "QuantaLogic",
  "license": "Apache-2.0"
}
