{
  "name": "qllm",
  "version": "0.5.0",
  "description": "QLLM Multi-Provider LLM Command CLI",
  "main": "dist/qllm.js",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev": "ts-node src/qllm.ts",
    "start": "node dist/qllm.js",
    "cli": "node dist/qllm.js"
  },
  "keywords": [
    "llm",
    "cli",
    "ai"
  ],
  "author": "quantalogic",
  "license": "Apache-2.0",
  "dependencies": {
    "@anthropic-ai/bedrock-sdk": "^0.10.0",
    "@types/node": "^20.14.8",
    "@types/prompts": "^2.4.9",
    "aws-sdk": "^2.1651.0",
    "cli-spinner": "^0.2.10",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "openai": "^4.52.2",
    "prompts": "^2.4.2",
    "winston": "^3.13.0"
  },
  "devDependencies": {
    "@types/cli-spinner": "^0.2.3",
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.[jt]s?(x)",
      "**/?(*.)+(spec|test).[tj]s?(x)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
